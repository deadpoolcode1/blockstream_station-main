{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Yocto",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/${config:MCONFIG.PROGRAM}",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "<path-to-gdb>",
        "miDebuggerArgs": "-ex 'target remote ${config:MCONFIG.TARGET_IP}:1234'",
        "preLaunchTask": "build",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  